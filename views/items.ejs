<html>
    <body>
        <h1>Items in category: <%= categoryName.charAt(0).toUpperCase() + categoryName.slice(1) %> </h1>
        <ul>
            <% items.forEach(item => { %>
                <li>ID: <%= item.id %> Name: <%= item.name %> Quantity: <%= item.quantity %> Price: <%= item.price %></li>
            <% }) %>
        </ul>
    </body>
</html>